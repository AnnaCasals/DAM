<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="data">
    <xs:simpleType>
      <xs:sequence>
        <xsd:restriction base = "xsd:string">
          <xsd:pattern value = "\d{2} / \d{2}/\d{4}"/>
        </xsd:restriction>
      </xs:sequence>
    </xs:simpleType>
  </xs:element>
</xs:schema>
